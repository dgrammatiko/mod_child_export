{
  "name": "childexport",
  "version": "1.0.1",
  "scripts": {
    "get-joomla": "node joomla-tools.mjs -i",
    "link": "joomla-tools -l",
    "build": "joomla-tools -b media_source",
    "release": "joomla-tools -r",
    "init": "joomla-tools -i && joomla-tools -l && joomla-tools -b media_source",
    "lint-css": "npx stylelint ./media_source/.",
    "lint-js": "npx eslint .",
    "lint": "npm run lint-css && npm run lint-js",
    "update:browserlist": "npx browserslist@latest --update-db"
  },
  "joomla-extensions": {
    "components": [],
    "libraries": [],
    "modules": {
      "administrator": [
        {"name": "childexport", "version": "1.0.1"}
      ],
      "site": []
    },
    "plugins": {
      "actionlog": [],
      "api-authentication": [],
      "authentication": [],
      "behaviour": [],
      "captcha": [],
      "content": [],
      "editors": [],
      "editors-xtd": [],
      "extension": [],
      "fields": [],
      "filesystem": [],
      "finder": [],
      "installer": [],
      "media-action": [],
      "multifactorauth": [],
      "privacy": [],
      "quickicon": [],
      "sampledata": [],
      "system": [],
      "task": [],
      "user": [],
      "webservices": [],
      "workflow": []
    },
    "templates": {
      "administrator": [],
      "site": []
    }
  },
  "joomlaVersion": "4.3.0",
  "devDependencies": {
    "@dgrammatiko/joomla-tools": "0.0.417",
    "eslint": "9.13.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-vue": "9.30.0",
    "stylelint": "16.10.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.8.1"
  },
  "dependencies": {
    "@dgrammatiko/joomla-tools": "^0.0.417"
  }
}
